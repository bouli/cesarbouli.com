{% set bg_image_url = url_for("static", filename="images/" + domain + "/photo.jpg") %}<!DOCTYPE html>
<html lang="pt">
<head>
	<script>
		//window.location.replace("https://www.youtube.com/watch?v=RJHx5Toxwzk"); //videoclipe
	</script>
	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-122379890-1"></script>
	<script>
	  window.dataLayer = window.dataLayer || [];
	  function gtag(){dataLayer.push(arguments);}
	  gtag('js', new Date());

	  gtag('config', 'UA-166762733-1');
	</script>
	<script src="https://code.jquery.com/jquery-1.11.3.js"></script>
	<meta charset="UTF-8">
	<link rel="apple-touch-icon" sizes="57x57" href="{{ url_for('static',filename="images/apple-icon-57x57.png") }}">
	<link rel="apple-touch-icon" sizes="60x60" href="{{ url_for('static',filename="images/apple-icon-60x60.png") }}">
	<link rel="apple-touch-icon" sizes="72x72" href="{{ url_for('static',filename="images/apple-icon-72x72.png") }}">
	<link rel="apple-touch-icon" sizes="76x76" href="{{ url_for('static',filename="images/apple-icon-76x76.png") }}">

	<link rel="apple-touch-icon" sizes="114x114" href="{{ url_for('static',filename="images/apple-icon-114x114.png") }}">
	<link rel="apple-touch-icon" sizes="120x120" href="{{ url_for('static',filename="images/apple-icon-120x120.png") }}">
	<link rel="apple-touch-icon" sizes="144x144" href="{{ url_for('static',filename="images/apple-icon-144x144.png") }}">
	<link rel="apple-touch-icon" sizes="152x152" href="{{ url_for('static',filename="images/apple-icon-152x152.png") }}">
	<link rel="apple-touch-icon" sizes="180x180" href="{{ url_for('static',filename="images/apple-icon-180x180.png") }}">

	<link rel="icon" type="image/png" sizes="192x192"  href="{{ url_for('static',filename="images/android-icon-192x192.png") }}">
	<link rel="icon" type="image/png" sizes="32x32" href="{{ url_for('static',filename="images/favicon-32x32.png") }}">
	<link rel="icon" type="image/png" sizes="96x96" href="{{ url_for('static',filename="images/favicon-96x96.png") }}">
	<link rel="icon" type="image/png" sizes="16x16" href="{{ url_for('static',filename="images/favicon-16x16.png") }}">

	<link rel="manifest" href="{{ url_for('static',filename="images/manifest.json") }}">

	<meta name="msapplication-TileColor" content="#000000">
	<meta name="msapplication-TileImage" content="{{ url_for('static',filename="images/ms-icon-144x144.png") }}">

	<meta name="theme-color" content="#000000">
	<meta name="description" content="{{ meta_text }}" />

	<meta property="og:locale" content="pt-br" />
	<meta property="og:url" content="{{ url }}" />
	<meta property="og:title" content="{{ meta_title }}" />
	<meta property="og:site_name" content="{{ meta_title }}" />
	<meta property="og:description" content="{{ meta_text }}" />
	<meta property="og:image" content="{{ meta_url + bg_image_url }}" />
	<meta property="og:image:type" content="image/png" />
	<meta property="og:image:width" content="948" />
	<meta property="og:image:height" content="424" />
	<meta property="og:type" content="website" />
	<link rel="preconnect" href="https://fonts.googleapis.com" />
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
	<link href="https://fonts.googleapis.com/css2?family=Cabin+Sketch&display=swap" rel="stylesheet" />
	<link rel="stylesheet" href="{{ url_for("static",filename="css/reset.css") }}" />
	<style>
		html {
			background: url("{{ bg_image_url }}") no-repeat center center fixed;
		}
	</style>
	<link rel="stylesheet" href="{{ url_for("static",filename="css/style.css") }}" />

	<title>{{ meta_title }}</title>

</head>
<body>
	{% if cta %}
	<div id="disco">
			<a alt="'{{ cta }}'" title="{{ cta }}'" href="{{ url_for(cta_url) }}" target="_blank"><img src="{{ url_for('static', filename="images/" + domain + "/cta.jpg") }}" /><br />
				{{ cta }}
			</a>
		<br />
	</div>
	{% endif %}
	<img src="{{ url_for('static',filename="images/" + domain + "/logo.png") }}" alt="{{ artist_name }}" class="logo" />
	<menu>
		{% if soundcloud_link %}
			<a alt="{{ artist_name }} - SoundCloud" title="Follow {{ artist_name }} on SoundCloud" href="{{ soundcloud_link }}" target="_blank"><img src="{{ url_for('static',filename="images/links-soundcloud.png") }}" /></a></br />
		{% endif %}
		{% if facebook_link %}
			<a alt="{{ artist_name }} - Facebook"   title="Follow {{ artist_name }} on Facebook" href="{{ facebook_link }}" target="_blank"><img src="{{ url_for('static',filename="images/links-facebook.png") }}" /></a></br />
		{% endif %}
		{% if instagram_link %}
			<a alt="{{ artist_name }} - Instagram"  title="Follow {{ artist_name }} on Instagram" href="{{ instagram_link }}" target="_blank"><img src="{{ url_for('static',filename="images/links-instagram.png") }}" /></a></br />
		{% endif %}
		{% if youtube_link %}
			<a alt="{{ artist_name }} - Youtube"    title="Follow {{ artist_name }} on Youtube" href="{{ youtube_link }}" target="_blank"><img src="{{ url_for('static',filename="images/links-youtube.png") }}" /></a></br />
		{% endif %}
		{% if spotify_link %}
			<a alt="{{ artist_name }} - Spotify"    title="Follow {{ artist_name }} on Spotify" href="{{ spotify_link }}" target="_blank"><img src="{{ url_for('static',filename="images/links-spotify.png") }}" />
		{% endif %}
	</menu>
</body>
</html>
